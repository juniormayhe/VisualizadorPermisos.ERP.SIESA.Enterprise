<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>Visualizador de Permisos de UNOEE</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css" integrity="sha384-PsH8R72JQ3SOdhVi3uxftmaW6Vc51MKb0q5P2rRUpPvrszuE4W1povHYgTpBfshb" crossorigin="anonymous">

    @Styles.Render("~/Content/css")
    
</head>
<body>
    <nav class="navbar navbar-inverse pl-5 fondo-azul">
        
        <a class="navbar-brand" href="#"><i class="fa fa-lock" aria-hidden="true"></i> Visualizador de permisos de ERP SIESA Enterprise UNOEE</a>
    </nav>
    
    <div class="container-fluid m-2">
        @RenderBody()
        <div class="centraliza">
            <div class="row">
                <footer class="mt-4 text-secondary font-weight-light">
                    <small>&copy; @DateTime.Now.Year <a href="http://github.com/juniormayhe" target="_blank">Junior Mayh&eacute;</a></small>
                </footer>
            </div>
        </div>
    </div>

    @*@Scripts.Render("~/bundles/jquery")*@
    @*@Scripts.Render("~/bundles/bootstrap")*@
    

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.2.1.min.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js" crossorigin="anonymous"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
