
@model IEnumerable<Model.Miembro>

@{
    Layout = null;
}
<select class="form-control w-100 h-100" id="miembro" size="10">
    
    @if (Model.Count() == 0)
    {
        
        <option value="" disabled>@(Request["area"] == null?"Seleccione una área":"No hay miembros")</option>
    }
    else
    {
        foreach (var area in Model)
        {
            <option value="@area.IdMiembro" disabled>@area.NombreMiembro</option>
        }
    }
    
</select>